{
  "name": "<%= appname %>",
  "version": "0.0.0",
  "description": "<%= appdescription %>",
  "license": "<%= applicense %>",
  "main": "",
  "repository":"<%= appname %>",
  "author": {
    "name": "<%= appauthor %>",
    "email": "<%= appemail %>",
    "url": ""
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "optimize": "gulp optimize",
    "deploy": "gulp deploy",
    "test": "mocha"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "browser-sync": "^2.11.2",
    "critical": "^0.7.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",<% if (useBabel == true) { %>
    "gulp-babel": "^6.1.1", <% } %>
    "gulp-beautify": "^2.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssimport": "^3.0.2",
    "gulp-imagemin": "^2.4.0",<% if (templateLang == 'jade') { %>
    "gulp-jade": "^1.1.0",<% } else { %>
    "gulp-htmlmin": "1.3.0", <% } %>
    "gulp-cssnano": "^2.1.0",<% if (jsLinter == 'jscs') { %>
    "gulp-jscs": "^3.0.2", <% } if (jsLinter == 'jshint') { %>
    "gulp-jshint": "^2.0.0", <% } %>
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",<% if (cssPrepro == 'less') { %>
    "gulp-less": "^3.0.5",<% } else { %>
    "gulp-sass":"^2.1.1",<% } %>
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-uncss": "^1.0.5",<% if (useFlexboxgrid) { %>
    "flexboxgrid": "^6.3.0",<% } %> <% if (useBootstrap) { %>
    "bootstrap": "^3.3.6",<% } %>
    "vinyl-ftp": "^0.4.5"
  },
  "devDependencies": {
    "mocha": "*"
  }
}
